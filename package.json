{
  "name": "@me3/keysmith",
  "version": "0.0.1",
  "description": "Me3 Key Generator Module",
  "keywords": [
    "me3",
    "keygen",
    "wallet",
    "avarta",
    "sso",
    "wallet",
    "crypto"
  ],
  "author": "Me3",
  "repository": {
    "type": "git",
    "url": "git@github.com:AvartaDev/Me3Keysmith.git"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/AvartaDev/Me3Keysmith",
  "bugs": {
    "url": "https://github.com/AvartaDev/Me3Keysmith/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "cli:dev": "ts-node cli/index.ts",
    "cli:prod": "NODE_OPTIONS=--openssl-legacy-provider ts-node cli/index.ts",
    "lint": "eslint . --ext .ts --fix",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@blitslabs/filecoin-js-signer": "^1.0.6",
    "@hapi/hapi": "^20.2.2",
    "@polkadot/keyring": "^10.1.8",
    "@polkadot/util": "^10.1.8",
    "@polkadot/util-crypto": "^10.1.8",
    "axios": "^0.27.2",
    "bip39": "^3.0.4",
    "bitcore-lib": "^8.25.38",
    "bitcore-lib-cash": "^8.25.38",
    "bitcore-lib-ltc": "^8.25.38",
    "ethers": "^5.7.1",
    "googleapis": "^108.0.0",
    "lodash": "^4.17.21",
    "qr-with-logo": "^1.0.5",
    "qrcode": "^1.5.1",
    "randomstring": "^1.2.2"
  }
}
