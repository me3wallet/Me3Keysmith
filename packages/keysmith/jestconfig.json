{
  "transform": {
    "^.+\\.(t|j)sx?$": [
      "ts-jest", {
        "isolatedModules": true
      }
    ]
  },
  "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "modulePathIgnorePatterns": [
    "<rootDir>/tests/fixtures"
  ],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "collectCoverageFrom": ["src/**/*.{ts,tsx}"],
  "collectCoverage":true,
  "testTimeout": 100000
}
