{
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "es2019",
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "./dist/**/*"
  ]
}
